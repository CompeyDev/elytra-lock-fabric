{
	"schemaVersion": 1,
	"id": "elytra-lock",
	"version": "${version}",
	"name": "elytra-lock",
	"description": "A simple mod which registers a keybind to lock your elytra from usage when not required.",
	"authors": [
		"DevComp"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/CompeyDev/elytra-lock"
	},
	"license": "CC0-1.0",
	"icon": "assets/elytra-lock/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"xyz.devcomp.elytralock.ElytraLock"
		],
		"modmenu": [
			"xyz.devcomp.elytralock.integrations.ModMenuIntegration"
		]
	},
	"mixins": [
		"elytra-lock.mixins.json"
	],
	"depends": {
